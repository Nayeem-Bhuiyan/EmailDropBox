@model IEnumerable<TeamMember>
@{
    ViewData["Title"] = "ListTeamMembers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Team Member Liast</h1>

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered">
            <thead>

            </thead>
            <tbody>
                @if (Model!=null)
                {
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.TeamMemberId</td>
                            <td>@item.Name</td>
                            <td>@item.Email</td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>

